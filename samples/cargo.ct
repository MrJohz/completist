["cargo.bench"]

[["cargo.bench".argument]]
kind = "seperator"

[["cargo.bench".argument]]
kind = "none"

[["cargo.bench".option]]
opt = "--lib"
description = "Benchmark only this package's library"
argument = "none"

[["cargo.bench".option]]
opt = "--bin"
argument = "command(ls target/debug)"
description = "Benchmark only the specified binary"
